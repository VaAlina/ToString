package toString;

import java.util.*;

public class toString {
	
	    private static int sumIt(int...a){
	    	return  a[0]+a[1]+a[2];
	    
	    }	
	    
	    private static int doJob(int[] a){
	    	a[0] =+ a[0]+1;	    	
	    	a[1] =+ a[1]+1;
	    	a[2] =+ a[2]+1;
	    	return a[0]+ a[1]+ a[2];
	    }
	    
	    public static void main(String[] args){
	    	
	    	int[] a = new int[]{1, 2, 3, 4, 6, 9};
	    	doJob(a);
	    	System.out.println(Arrays.toString(a));
	    	System.out.println(sumIt());
	    }
   }

ToString_1.1 25.06
========

package toString;

import java.util.*;

public class toString {
      	public static void main(String[]args){
			Scanner scanner = new Scanner(System.in);

			String x;
			int y;
			double z;

			x = scanner.nextLine();
			y = scanner.nextInt();
			z = scanner.nextDouble();

			System.out.print(x+ Integer.toString(y)+ Double.toString(z));

			}

}


ToString_1.0 24.06
========

import java.util.*;
public class string {

	public static void main(String[]args){
		Scanner scanner = new Scanner(System.in);
		
		String x;
		int y;
		double z;
		
		x = scanner.nextLine();
		y = scanner.nextInt();
		z = scanner.nextDouble();
		
		System.out.print(x+" "+ Integer.toString(y)+" "+ Double.toString(z));
		
	}
	
}
